<UserControl
    x:Class="Demo.Module.SimpleView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:UserControls="clr-namespace:Demo.UserControls;assembly=Demo.UserControls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Demo.Module"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:prism="http://www.codeplex.com/prism"
    Width="auto"
    Height="auto"
    Background="Aqua"
    mc:Ignorable="d">
    <Grid>
        <UserControls:SimpleUserControl
            SimpleChoiceList="{Binding SimpleChoiceList}"
            SimpleDoSomething="{Binding SimpleDoSomethingCommand}"
            SimpleText="{Binding SimpleText}" />


    </Grid>
</UserControl>
